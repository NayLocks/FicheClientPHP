{% set nbSection = 1 %}
<html xmlns="http://www.w3.org/1999/html">
<head>
    <style type="text/css">

        body {
            font-family: 'arial';
            margin: 0.5cm 0;
            text-align: justify;
            font-size: 15px;
            color: #6D6F71;
        }

        #header{
            position: relative;
            top: -30px;
            left: 0;
            right: 0;
        }

        #footer {
            position: fixed;
            left: 0;
            right: 0;
            font-size: 0.9em;
        }

        #footer {
            bottom: 60px;
        }

        #header table,
        #footer table {
            width: 90%;
            border-collapse: collapse;
            border: none;
        }

        #header td,
        #footer td {
            padding: 0;
            width: 50%;
        }

        .page-number {
            text-align: right;
        }

        .page-number:before {
            content: "Page " counter(page);
        }

        hr {
            page-break-after: always;
            border: 0;
        }
        .colorSystem{
            color: #1F497D;
        }

        tr {
            border: 1px solid black
        }
        .gray{
            background-color: lightgray;
            color: black;
            font-weight: bold;
        }
        .black{
            
        }

        .colorVERT{
            background-color: #70C057;
        }

    </style>
